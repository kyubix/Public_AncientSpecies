<Defs>
<!--==============================================PC Settings==============================================-->
  <PawnKindDef ParentName="AS_WHE_CommonBase">
    <defName>AS_WHE_Colonist</defName>
    <label>Harf Elf Colonist</label>
    <defaultFactionType>AS_WHE_PlayerFaction_Neolithic</defaultFactionType>
    <isFighter>false</isFighter>
    <backstoryCategories>
      <li>AS_WHE_Villager</li>
    </backstoryCategories>
    <apparelTags>
      <li>AS_WHE_Colonist</li>
    </apparelTags>
    <apparelRequired>
      <li>AS_WHE_Tribal</li>
    </apparelRequired>
  </PawnKindDef>

<!--==============================================NPC Settings==============================================-->
  <!-- villager -->
  <PawnKindDef ParentName="AS_WHE_NPCBase_Neolithic">
    <defName>AS_WHE_Villager</defName>
    <label>Half Elf villager</label>
    <itemQuality>Normal</itemQuality>
    <isFighter>false</isFighter>
    <weaponMoney>80~180</weaponMoney>
    <weaponTags>
      <li>NeolithicRangedBasic</li>
    </weaponTags>
    <apparelTags>
      <li>WHE_Neolithic</li>
    </apparelTags>
    <apparelAllowHeadgearChance>0.2</apparelAllowHeadgearChance>
    <backstoryCategories>
      <li>AS_WHE_Villager</li>
    </backstoryCategories>
    <inventoryOptions>
      <skipChance>0.4</skipChance>
      <subOptionsChooseOne>
        <li>
          <choiceChance>100</choiceChance>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>100</min>
            <max>200</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
  </PawnKindDef>

