<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- Check if Ancient Species mod exists -->
	<Operation Class="PatchOperationFindMod">
		<success>Always</success>
		<mods>
	<!-- Check Mod name or workshop ID written in PublishedFileId.txt (Confirm existence of other Mod from this Mod) -->
			<li>[1.1] DE Surgeries</li>
			<li>[1.1] DE Surgeries Color</li>
			<li>[1.1] DE Surgeries for RBSE only</li>
			<li>[1.1] DE Surgeries for RBSE only Color</li>
			<li>[1.1] DE Surgeries Hard</li>
			<li>1541722340</li>
			<li>2002422428</li>
			<li>1541914821</li>
			<li>1983507181</li>
			<li>1556888077</li>
			<li>proxyer.desurgeries.desurgeries</li>
			<li>proxyer.desurgeries.desurgeriescolor</li>
			<li>proxyer.desurgeries.rbseonly</li>
			<li>proxyer.desurgeries.rbseonlycolor</li>
			<li>proxyer.desurgeries.hard</li>
		</mods>
	<!-- Add modded race Ancient Species ThingDef race Defname to this Mod -->
		<match Class="PatchOperationAdd">
			<xpath>/Defs/RecipeDef[@Name = "DESurgeries"]/recipeUsers</xpath>
			<value>
				<li>AS_WHE_Race</li>
			</value>
		</match>
	</Operation>

</Patch>